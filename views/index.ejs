    .photo {
      display: block;
      margin: 0;
      height: 100%;
    }

    #navbar {
        background-color:rgba(25,25,25,0.9);
        background-size: cover;
        box-shadow: 8px 1px 15px #000;
        position: fixed;
        width: 150px;
        height: 100%;
    }

    #navbar h1 {
      color: white;
      text-align: right;
      margin-right: 5px;
    }

    #navbar .navLinks {
      margin-top: 30%;
      text-align: right;
    }

    #navbar p {
      font-size: 30px;
      color: #fff;
      font-family: 'Roboto Condensed', sans-serif;
      padding: 10px;
      margin-top: 40px;
      transition-duration: 0.3s;
    }

    #navbar p:hover {
      color: #0080ff;
      transition-duration: 0.3s;
    }

    #navbar .icons {
      margin-left: 3px;
      margin-top: 30%;
    }

    #navbar i {
      color: white;
      font-size: 40px;
      margin: 10px;
    }



    #picture_1 {
      background: url('/images/IMG_5585.JPG') no-repeat;
      background-attachment: fixed;
      top: 0;
      left: 0;
      width: 100%;
    }

