    <script src="/javascripts/jquery.stellar.js"></script>
    <script type='text/javascript'>
      $(document).ready(function() {
        $(window).stellar();

        $.stellar({
          horizontalOffset: 0,
          verticalOffset: 0,

          scrollProperty: 'scroll',

          horizontalscrolling: false,
          verticalscrolling: false
        });

        $('table').stellar({
          scrollProperty: 'scroll'
        });
      })

    </script>
